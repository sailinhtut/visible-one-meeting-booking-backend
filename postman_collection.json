      "name": "Auth",
      "item": [
        {
          "name": "Register",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "let response = pm.response.json();\r",
                  "\r",
                  "if (response.access_token) {\r",
                  "    pm.globals.set(\"token\", response.access_token);\r",
                  "    console.log(\"Token saved to global variable:\", response.access_token);\r",
                  "} else {\r",
                  "    console.warn(\"Token not found in the response payload.\");\r",
                  "}\r",
                  ""
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [],
            "body": {
